{
  "name": "@jmdc/rehooks",
  "version": "0.1.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0",
    "yarn": "^1.17.3"
  },
  "files": [
    "package.json",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "yarn run build:js && yarn run build:types",
    "build:js": "BABEL_ENV=production babel src -d dist --extensions '.ts,.tsx' --ignore '**/*.spec.tsx,**/*.stories.tsx'",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist",
    "karma": "karma",
    "lint": "eslint . --ext '.js,.ts,.tsx'",
    "prebuild": "yarn clean",
    "test": "yarn jest",
    "test:karma": "karma start --single-run",
    "test:karma:chrome": "karma start --single-run --browsers Chrome",
    "test:karma:firefox": "karma start --single-run --browsers Firefox",
    "test:karma:edge": "karma start --single-run --browsers Edge",
    "test:karma:ie": "karma start --single-run --browsers ie_no_addons",
    "test:karma:safari": "karma start --single-run --browsers Safari"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "dependencies": {
    "core-js": "^3.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@jmdc/eslint-config-typescript-react": "^0.11.0",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jasmine-enzyme": "^7.0.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.20",
    "@types/react": "^16.9.16",
    "@types/selenium-webdriver": "^4.0.6",
    "@types/webpack": "^4.41.0",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "jasmine": "^3.5.0",
    "jasmine-enzyme": "^7.1.2",
    "jest": "^24.9.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.2.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "puppeteer": "^2.0.0",
    "rimraf": "^3.0.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "typescript": "^3.7.3",
    "webpack": "^4.41.3"
  }
}
